{
  "$id": "https://dainty-vscode.now.sh/configuration-schema.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "variant": {
      "$id": "#/properties/variant",
      "type": "string",
      "enum": ["dark", "light"],
      "default": "dark"
    },
    "colors": {
      "$id": "#/properties/colors",
      "type": "object"
    },
    "customizations": {
      "$id": "#/properties/customizations",
      "type": "object"
    }
  }
}
